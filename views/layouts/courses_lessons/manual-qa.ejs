<!DOCTYPE html>
<html lang="en">

<head>
  <title>QA.Live.Study</title>
  <link rel="stylesheet" href="/css/reboot.css" />
  <link rel="stylesheet" href="/css/courses/course-qa.css" />
  <link rel="stylesheet" href="/css/support/button-up.css" />
  <link href="https://fonts.googleapis.com/css?family=Montserrat:400,500,600,700,800,900&display=swap" rel="stylesheet" />
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" />
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" />
  <link rel="stylesheet" href="<%= assetFingerprint('/css/main.css') %>" />
  <link rel="stylesheet" href="<%= assetFingerprint('/css/style.css') %>" />
</head>

<body>

  <section class="section-manual">
    <div class="container">
      <h1 class="section-header pt-4 pb-4 text-center">
        Мануальное тестирование ПО
      </h1>
      <div class="text-center pb-5 d-flex justify-content-around">
        <a href="/" class="btn btn-outline-primary">Главная</a>
        <a href="/reg" class="btn btn-outline-warning">Регистрация</a>
        <a href="/login" class="btn btn-outline-success">Вход</a>
      </div>
      <div class="row">
        <div class="col-md-8 col-sm-12  mb-5 text-center border course-logo">
          <img src="/img/section-courses/manual-course.png" class="logo-img" alt="" />
        </div>

        <div class="col-md-4 col-sm-12 mb-5">
          <!-- Card -->
          <div class="course-about">
            <!-- Card image -->
            <div class="gdlr-lms-course-info-inner-wrapper">
              <img class="mb-4 card-img-top rounded-circle" data-holder-rendered="true"
                src="/img/courses-pages/author/pinchuk_da.jpg" alt="Author photo" />
            </div>
            <div class="course-info">
              <div class="gdlr-lms-info">
                <span class="head">Преподаватель:&nbsp;&nbsp;&nbsp;</span>
                <span class="tail"> Дмитрий Пинчук</span>
              </div>
              <div class="gdlr-lms-info">
                <span class="head">Тип:&nbsp;&nbsp;&nbsp;</span>
                <span class="tail"> Онлайн-курс</span>
              </div>
              <div class="gdlr-lms-info">
                <span class="head">Обучается студентов:&nbsp;&nbsp;&nbsp;</span>
                <span class="tail"> 25</span>
              </div>
              <div class="gdlr-lms-rating-wrapper">
                <i class="fa fa-star icon-star"></i>
                <i class="fa fa-star icon-star"></i>
                <i class="fa fa-star icon-star"></i>
                <i class="fa fa-star icon-star"></i>
                <i class="fa fa-star icon-star"></i>
                <span class="gdlr-lms-rating-amount">(1 Рейтинги)</span>
              </div>
            </div>
            <div class="course-button">
              <a href="/login" class="btn btn-primary text-uppercase">подписаться</a>
            </div>
          </div>
        </div>
      </div>

      <% let i = 0 %>
      <% let coursesLessonsDescriptions = [
          {
              lessonName: "Занятие #1. История возникновения тестирования",
              lections: [
                  "Введение",
                  "Конспект лекции",
                  "Тесты"
              ]
          },

          {
              lessonName: "Занятие #2. Введение в тестирование",
              lections: [
                  "Введение",
                  "Конспект лекции",
                  "Тесты"
              ]
          },

          {
              lessonName: "Занятие #3. Виды тестирования",
              lections: [
                  "Введение",
                  "Конспект лекции",
                  "Тесты"
              ]
          },

          {
              lessonName: "Занятие #4. Тестовая документация",
              lections: [
                  "Введение",
                  "Конспект лекции",
                  "Тесты"
              ]
          },

          {
              lessonName: "Занятие #5. Практика по написанию тест-кейсов",
              lections: [
                  "Введение",
                  "Конспект лекции",
                  "Тесты",
                  "Домашнее задание"
              ]
          },

          {
              lessonName: "Занятие #6. Тест-дизайн",
              lections: [
                  "Введение",
                  "Конспект лекции",
                  "Тесты"
              ]
          },

          {
              lessonName: "Занятие #7. Практика по написанию баг-репортов",
              lections: [
                  "Введение",
                  "Конспект лекции",
                  "Тесты",
                  "Домашнее задание"
              ]
          },

          {
              lessonName: "Занятие #8. Web и клиент-серверная архитектура",
              lections: [
                  "Введение",
                  "Web",
                  "Клиент-серверная архитектура",
                  "Протоколы запросов",
                  "Методы протоколов",
                  "Коды состояния",
                  "Заголовки",
                  "Тесты"
              ]
          },

          {
              lessonName: "Занятие #9. Практическое занятие по dev-tools и firebug",
              lections: [
                  "Введение",
                  "Практика",
                  "Тесты",
                  "Домашнее задание"
              ]
          },

      ];













      coursesLessonsDescriptions.forEach(courseLessons => { %>
        <div class="row pb-5">
          <div class="col-12">
            <div class="course-program-wrapper">
              <div class="row cursor-pointer">
                <div class="col-12 course-program-item">
                  <i class="fa fa-graduation-cap circle mr-1"></i>
                  <div class="pl-0">
                    <div class="font-bold">
                      <span class="title"><%= courseLessons.lessonName %></span>
                    </div>

                    <% courseLessons.lections.forEach(lection => { %>
                      <p class="mb-0">
                        <i class="fa fa-code"></i><span><%= lection %></span>
                      </p>
                    <% }) %>

                  </div>
                </div>
              </div>
            </div>
            <% i++ %>
          <% }) %>

    </div>
        </div>
    </div>
  </section>

  <button class="scroll-to-top-btn"><i class="fas fa-arrow-up"></i></button>

  <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.4.1.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.0/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>
  <script src="/js/backtotop.js"></script>
  <script src="<%= assetFingerprint('/js/lessons-descriptions/courses-lessons-descriptions.js') %>"></script>
</body>

</html>